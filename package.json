{
  "name": "specjs",
  "version": "0.0.1",
  "description": "Design by contract javascript library",
  "keywords": [
    "contract",
    "offensive",
    "correctness",
    "precondition",
    "postcondition",
    "invariant",
    "test",
    "require",
    "ensure"
  ],
  "source": "src/specjs.ts",
  "main": "dist/specjs.js",
  "module": "dist/specjs.esm.js",
  "unpkg": "dist/specjs.umd.js",
  "browser": "dist/specjs.umd.js",
  "types": "dist/specjs.d.ts",
  "scripts": {
    "test": "standardx --fix && tsc --noEmit && jest --coverage",
    "test:watch": "jest --watch",
    "build": "microbundle -i src/specjs.ts -o dist --name SpecJS"
  },
  "author": "Alfonso Filho <alfonsocfilho@gmail.com>",
  "homepage": "https://github.com/AlfonsoFilho/specjs#readme",
  "bugs": "https://github.com/AlfonsoFilho/specjs/issues",
  "repository": "github:AlfonsoFilho/specjs",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "jest": "25.2.3",
    "microbundle": "0.12.0-next.8",
    "standardx": "5.0.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "dependencies": {},
  "runkitExampleFilename": "examples/example.js"
}
